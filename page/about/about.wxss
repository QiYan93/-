.page-about-wrapper .info {
  padding-top: 200rpx;
  text-align: center; }
  .page-about-wrapper .info .logo {
    width: 81px;
    height: 81px;
    margin: 0 auto; }
  .page-about-wrapper .info .name {
    line-height: 40px;
    font-size: 18px;
    font-weight: bold; }
  .page-about-wrapper .info .desc {
    color: #999; }

.page-about-wrapper .contact {
  position: absolute;
  left: 50%;
  bottom: 200rpx;
  width: 100px;
  margin-left: -50px;
  color: #fff;
  background: #1296db; }

.page-about-wrapper .thanks {
  position: absolute;
  bottom: 15px;
  width: 100%;
  padding: 0 30px;
  text-align: center;
  box-sizing: border-box; }
  .page-about-wrapper .thanks .line {
    line-height: 20px;
    color: #999;
    font-size: 12px; }
    .page-about-wrapper .thanks .line .href {
      color: #1296db;
      text-decoration: underline; }
